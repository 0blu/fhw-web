[{
  "url": "/assets/*",
  "static": "assets/*"
},{
  "url": "/assets/*",
  "static": "assets/*"
}, {
  "url": "/impressum",
  "params": {
    "get": ["currency", "sortBy"],
    "post": ["mk"]
  },
  "controller": {
    "file": "impressum",
    "function": "validate"
  }
}, {
  "url": "/guestbook",
  "method": ["post"],
  "controller": {
    "file": "guestbook",
    "function": "add"
  },
  "params": {
    "get": ["id"],
    "post": ["author", "text"]
  }
}, {
  "url": "/if-basics",
  "method": ["get"],
  "page": "if-basics"
},  {
  "url": "/catalogue",
  "method": ["get"],
  "page": "sub/catalogue"
},   {
  "url": "/*",
  "method": ["get"],
  "page": "*"
}, {
  "url": "/foo2/*",
  "method": ["get"],
  "controller": {
    "file": "not-found",
    "function": "renderError"
  }
}]