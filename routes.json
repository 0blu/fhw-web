[{
  "url": "/assets/*",
  "static": "public/*"
},{
  "url": "/assets/*",
  "static": "assets/*"
}, {
  "url": "/impressum/:date",
  "page": "impressum"
}, {
  "url": "/guestbook",
  "method": ["post"],
  "controller": {
    "file": "guestbook",
    "function": "add"
  },
  "params": {
    "get": ["id"],
    "post": ["author", "text"]
  }
}, {
  "url": "/if-basics",
  "method": ["get"],
  "page": "if-basics"
},  {
  "url": "/catalogue",
  "method": ["get"],
  "page": "sub/catalogue"
},   {
  "url": "/*",
  "method": ["get"],
  "page": "*"
}, {
  "url": "/*",
  "method": ["get"],
  "controller": {
    "file": "not-found",
    "function": "renderError"
  }
}]